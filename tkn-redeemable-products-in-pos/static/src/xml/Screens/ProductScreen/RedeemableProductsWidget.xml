<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" xml:space="preserve">

    <t t-name="RedeemableProductsWidget" owl="1">
      <div class="redeemable-products-widget">
            <div class="redeemable-products-control-panel">
              <RedeemableProductsWidgetControlPanel mobileSearchBarIsShown="props.mobileSearchBarIsShown" breadcrumbs="breadcrumbs" subcategories="subcategories" hasNoCategories="hasNoCategories" />
            </div>
            <div class="redeemable-products-list">
              <RedeemableProductList products="productsToDisplay" searchWord="searchWord" />
            </div>
        </div>
    </t>

</templates>
